@using CapaServicio;
@model  Sedes
@{
    ViewBag.Title = "Gestión de Sedes-Nueva Sede";

}

<div class="container-fluid">
    <div class="col-md-6 col-md-offset-3">

        @using (Html.BeginForm("EditarSede", "Administracion", FormMethod.Post))
        {
            @Html.HiddenFor(u => u.IdSede)
            <div class="split style2">
                <section>
                    <h2>Editar Sede</h2>
                </section>
                @Html.Partial("CrearEditarSedePartial")

            </div>

        }
    </div>
   </div>
